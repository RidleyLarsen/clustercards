<?xml version='1.0' encoding='utf-8'?>
<widget id="xyz.ridley.cardgroups" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>ClusterCards</name>
    <description>
        A game 'bout makin' groups of cards.
    </description>
    <author email="hello@ridley.xyz" href="https://ridley.xyz/">
        Ridley Larsen
    </author>
    <icon density="mdpi" height="57" platform="ios" src="platforms/android/res/mipmap-xhdpi/icon.png" width="57" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-minSdkVersion" value="23" />
</widget>
